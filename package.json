    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^3.0.0",
    "eslint": "^5.14.1",
    "mocha": "^3.5.3",
    "nodemon": "^1.18.10"
  }
